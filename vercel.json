{
  "version": 2,
  "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/api"
      },
      {
          "source": "/(.*)",
          "destination": "/components"
      }
  ],
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
{
  "version": 2,
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/components/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/components/$1"
    }
  ]
}